{
	"name": "Change Request Header",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Change Http/Https Request Header!",
	"icons": { "128": "icon.png","64":"icon.png" },
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["./js/background.js"]
	},
	"permissions": [
		"tabs", 
		"webRequestBlocking", 
		"webRequest", 
		"<all_urls>"
	],
	"web_accessible_resources": [
		"icon.png"
	]
}
